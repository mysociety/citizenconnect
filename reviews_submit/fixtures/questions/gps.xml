  <entry>
    <id>2</id>
    <title type="text">Are you able to get through to the surgery by telephone?</title>
    <updated>2013-04-24T16:02:53Z</updated>
    <Answer xmlns="http://syndication.nhschoices.nhs.uk/schemas/answer" id="1">Never</Answer>
    <Answer xmlns="http://syndication.nhschoices.nhs.uk/schemas/answer" id="2">Rarely</Answer>
    <Answer xmlns="http://syndication.nhschoices.nhs.uk/schemas/answer" id="3">Sometimes</Answer>
